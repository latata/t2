{
  "name": "t2",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.0.0",
    "immutable": "^4.0.0-rc.9",
    "jquery": "^3.3.1",
    "lodash.get": "^4.4.2",
    "material-ui": "^1.0.0-beta.38",
    "node-sass-chokidar": "^1.2.1",
    "normalize.css": "^8.0.0",
    "npm-run-all": "^4.1.2",
    "react": "^16.3.1",
